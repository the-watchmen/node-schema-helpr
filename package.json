  "license": "ISC",
  "peerDependencies": {
    "config": ">=1",
    "debug": ">=3",
    "joi": ">=13",
    "lodash": ">=4",
    "prop-types": ">=15",
    "react": ">=16",
    "react-dom": ">=16",
    "react-redux": ">=5",
    "redux": ">=3",
    "redux-form": ">=7.0.0"
  },
  "dependencies": {
    "@watchmen/helpr": ">=1.1"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": ">=1",
    "ava": ">=0.24",
    "babel-cli": ">=6.24",
    "babel-eslint": ">=8",
    "babel-preset-env": ">=1.6",
    "babel-preset-react": ">=6.24",
    "babel-preset-stage-0": ">=6.24",
    "cross-env": ">=5",
    "eslint-config-prettier": ">=2",
    "eslint-config-xo-react": ">=0.14",
