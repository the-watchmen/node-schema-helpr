{
  "name": "@watchmen/schema-helpr",
  "version": "1.1.1",
  "description": "",
  "main": "dist/index.js",
  "repository": "the-watchmen/node-schema-helpr",
  "scripts": {
    "ava": "yarn ava-single test/ava",
    "ava-single": "cross-env DEBUG=test:*,app:*,lib:* NODE_ENV=auto ava -s",
    "pretest": "xo",
    "test": "npm run ava",
    "prebuild": "npm run test",
    "build": "rm -rf dist && babel src -d dist",
    "fix": "xo --fix",
    "prepare": "install-self-peers -- --ignore-scripts"
  },
  "author": "tony kerz",
  "license": "ISC",
  "peerDependencies": {
    "debug": ">=3",
    "lodash": ">=4"
  },
  "dependencies": {
    "@watchmen/helpr": "^2.1.0"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": ">=1",
    "ava": ">=0.24",
    "babel-cli": ">=6.24",
    "babel-eslint": ">=8",
    "babel-preset-env": ">=1.6",
    "babel-preset-react": ">=6.24",
    "babel-preset-stage-0": ">=6.24",
    "cross-env": ">=5",
    "eslint-config-prettier": ">=2",
    "eslint-plugin-prettier": "^2.4.0",
    "joi": ">=13",
    "joi-to-json-schema": ">=3",
    "prettier": ">=1",
    "xo": ">=0.19"
  },
  "ava": {
    "babel": "inherit",
    "require": ["babel-register", "babel-polyfill"]
  },
  "xo": {
    "useEslintrc": true
  },
  "publishConfig": {
    "access": "public"
  }
}
